{
    "name": "phpeel/banana-validator",
    "description": "Various value validation library.",
    "license": "MIT",
    "keywords": [
        "validation",
        "validator",
        "validate"
    ],
    "homepage": "https://github.com/phpeel/banana-validator",
    "authors": [
        {
            "name" : "Banana Validator Contributors",
            "homepage": "https://github.com/phpeel/banana-validator/contributors"
        }
    ],
    "require": {
