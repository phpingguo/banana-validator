        "phpeel/citron-di": "dev-develop",
        "phpeel/apricot-lib": "dev-develop"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "squizlabs/php_codesniffer": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Phpeel\\BananaValidator\\": "src/",
            "Phpeel\\BananaValidator\\Test\\": "tests/src/"
        }
